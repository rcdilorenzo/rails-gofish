#myDrawingDiv
  %canvas#drawing{:width => "940px", :height => "800px", :data => {save: '/js-endgame', :token => "#{form_authenticity_token}"}, :style => "position:absolute; z-index: 1; margin-top: -20px; margin-left:110px;", :"data-screenname" => "#{@name}"}
  - ranks = [2, 3, 4, 5, 6, 7, 8, 9, 10, 'Jack', 'Queen', 'King', 'Ace']
  - @imgStrings.each do |imgString|
    %img{"src" => "#{imgString}", "style" => "display:none;", :width => "71px", :height => "96px", "id" => "#{imgString[15..-5]}", "data-rank" => "#{ranks[@imgStrings.index(imgString)/4]}"}
